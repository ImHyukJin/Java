package Programers;
import java.util.*;
class Solution {
    public ArrayList<Integer> solution(int[] numbers) {
       
        
        ArrayList<Integer> list = new ArrayList<>();
        int min = 0;
        int now = 0;
        for(int i = 0 ; i < list.size(); i++){
            if(now<=numbers[i]){
                list.add(now);
                continue;
            }
            if(min<=numbers[i]){
                list.add(-1);
                continue;
            }
            for(int j = i ; j<list.size(); j++){
                if(numbers[i]<numbers[j]){
                    list.add(numbers[j]);
                        now=numbers[j];
                        break;
                    }
                if(j== list.size()-1){
                    list.add(-1);
                    min=numbers[i];
                }
            }
            
        }
        return list;
        //뒷수가 앞수보다 작으면 같은 값 -- 시간줄여야 할듯?
        //list에 담기 
        //-1이 뜨는 가장 작은 값 min으로 하고 그보다 큰 값 다 -1
        
        
    }
}