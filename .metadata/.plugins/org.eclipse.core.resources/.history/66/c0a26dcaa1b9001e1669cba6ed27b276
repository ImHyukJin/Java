package Programers;


import java.util.*;
class Solution {
    public ArrayList<Integer> solution(int[] numbers) {
       
        
        ArrayList<Integer> list = new ArrayList<>();
        int min = 1000001;
        int now = 0;
        
        for(int i = 0 ; i < numbers.length; i++){
             if(min<=numbers[i]){
                list.add(-1);
                continue;
            }
            if(!list.isEmpty() && numbers[now]==numbers[i]){
                list.add(list.get(list.size()-1));
                continue;
            }
    
           
            for(int j = i ; j<numbers.length; j++){
                if(numbers[i]<numbers[j]){
                    list.add(numbers[j]);              
                    now = i;
                    break;
                    }
                if(j==numbers.length-1){
                    list.add(-1);
                    min=numbers[i];
                }
            }
            
        }
       
        return list;

        //-1이 뜨는 가장 작은 값 min으로 하고 그보다 큰 값 다 -1
        
        
    }
}