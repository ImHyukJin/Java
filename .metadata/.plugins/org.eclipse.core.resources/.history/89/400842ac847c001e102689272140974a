package quiz1;

import java.util.Arrays;
import java.util.Scanner;

public class Quiz12 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] a = new int[n];
		for(int i=0;i<n;i++) {
			a[i] = i+1 ;
		}System.out.println(Arrays.toString(a));
	}
}
