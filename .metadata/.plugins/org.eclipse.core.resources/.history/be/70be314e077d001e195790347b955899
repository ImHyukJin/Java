package day04;

public class MethodEx04 {

	public static void main(String[] args) {
		
		//배열을 매개변수로 전달?
		String[] arr = {"월", "화", "수"};
		
		String result = func01(arr);
		System.out.println(result);
		
		//배열을 반환?
	}
	
	//배열을 매개변수로 받는다.
	static String func01(String[] arr) {
		
		String str = "";
		for(int i = 0; i < arr.length; i++) {
			str += arr[i];
		}
		return str;
	}
	
	
	
	
	
	
	
}
